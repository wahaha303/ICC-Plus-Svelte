{
  "name": "iccplus_viewer",
  "private": true,
  "version": "2.8.9",
  "type": "module",
  "scripts": {
    "dev": "vite",
    "build": "vite build && vite build --config vite.config.local.ts && node build-core.js && node build-index.js && node add-comment.js && node build-local-index.js",
    "preview": "vite build && node build-core.js && node build-index.js && node add-comment.js && vite preview",
    "prepare": "npm run smui-theme-light && npm run smui-theme-dark",
    "smui-theme-light": "smui-theme compile public/css/smui.css -i src/theme",
    "smui-theme-dark": "smui-theme compile public/css/smui-dark.css -i src/theme/dark",
    "check": "svelte-check --tsconfig ./tsconfig.app.json && tsc -p tsconfig.node.json",
    "postinstall": "patch-package"
  },
  "devDependencies": {
    "@sveltejs/vite-plugin-svelte": "^6.2.0",
    "@tsconfig/svelte": "^5.0.4",
    "@types/canvas-size": "^1.2.2",
    "@types/node": "^24.0.12",
    "@types/youtube": "^0.1.2",
    "@vitejs/plugin-legacy": "^7.2.1",
    "core-js": "^3.44.0",
    "smui-theme": "^8.0.3",
    "svelte-check": "^4.3.1",
    "terser": "^5.43.1",
    "typescript": "~5.8.3",
    "vite": "^7.1.5"
  },
  "dependencies": {
    "@antv/expr": "^1.0.2",
    "@smui/button": "^8.0.3",
    "@smui/common": "^8.0.3",
    "@smui/dialog": "^8.0.3",
    "@smui/drawer": "^8.0.3",
    "@smui/form-field": "^8.0.3",
    "@smui/icon-button": "^8.0.3",
    "@smui/list": "^8.0.3",
    "@smui/paper": "^8.0.3",
    "@smui/select": "^8.0.3",
    "@smui/slider": "^8.0.3",
    "@smui/snackbar": "^8.0.3",
    "@smui/switch": "^8.0.3",
    "@smui/tab": "^8.0.3",
    "@smui/tab-bar": "^8.0.3",
    "@smui/textfield": "^8.0.3",
    "@smui/tooltip": "^8.0.3",
    "@smui/top-app-bar": "^8.0.3",
    "bootstrap": "^5.3.7",
    "cropperjs": "^1.6.2",
    "dompurify": "^3.2.6",
    "html-to-image": "^1.11.13",
    "patch-package": "^8.0.0",
    "propper": "^1.0.4",
    "resize-observer-polyfill": "^1.5.1",
    "ress": "^5.0.2",
    "svelte": "^5.38.9",
    "svelte-material-ui": "^8.0.3",
    "zod": "^3.25.67"
  }
}
